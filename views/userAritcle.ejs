<% include header.ejs %>
<% posts.forEach(function (post, index) { %>
  <p><h2><a href="/u/<%= post.username %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
  <p>
    作者：<a href="/u/<%= post.username %>"><%= post.name %></a> | 
    日期：<%= post.time.minute %>
  </p>
  <p><%- post.post %></p>
<% }) %>
<% include paging.ejs %>
<% include footer.ejs %>